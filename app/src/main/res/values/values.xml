<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="CustomToolbar">
        <attr name="title" format="string" />
        <attr name="leftTitle" format="string" />
        <attr name="leftTitleIcon" format="boolean" />
        <attr name="leftLogo" format="reference|color" />
        <attr name="bottomLine" format="boolean" />
        <attr name="rightTitle" format="string" />
        <attr name="rightBtnEnable" format="boolean" />
        <attr name="rightLogo" format="reference|color" />
    </declare-styleable>

    <dimen name="design_bottom_navigation_active_text_size" tools:override="true">10sp</dimen>
    <dimen name="design_bottom_navigation_text_size" tools:override="true">10sp</dimen>

</resources>